  <resource uri="http://example.org/striped">
    <a><ex:Striped/></a>
  </resource>
  <resource uri="http://example.org/2009/example_001/otherthing">
    <a><ex:Other/></a>
    <a><Special xmlns="http://example.org/ns2/"/></a>
    <a><Tag xmlns="tag:example.org,2009:ns:"/></a>
    <ex:value8>otherthing</ex:value8>
  </resource>
  <resource uri="http://example.org/2009/example_001/void">
    <a><ex:Void/></a>
    <a><ex:None/></a>
    <ex:value9>void</ex:value9>
    <ex:value3 fmt="datatype">
      <odd xmlns="http://example.org/other#">odd</odd>
    </ex:value3>
  </resource>
