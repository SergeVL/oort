<?xml version="1.0"?>
<graph xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
       xmlns:ex="http://example.org/ns#"
       xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
  <resource uri="http://example.org/2009/example_001">
    <a><ex:Thing/></a>
    <ex:value1>plain</ex:value1>
    <ex:value2 xml:lang="en">English</ex:value2>
    <ex:value3 fmt="datatype">
      <xsd:string>typed</xsd:string>
    </ex:value3>
