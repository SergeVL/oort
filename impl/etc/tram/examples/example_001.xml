    </ex:value4>
    <ex:rel1 ref="http://example.org/2009/example_001/otherthing"/>
    <ex:rel2>
      <a><ex:Anonymous/></a>
      <ex:value5>anonymous 1</ex:value5>
    </ex:rel2>
    <ex:list1>
      <li>
        <ex:value6>anonymous 2</ex:value6>
      </li>
      <li>
        <ex:value7>anonymous 3</ex:value7>
      </li>
